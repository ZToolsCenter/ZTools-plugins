{
  "main": "index.html",
  "preload": "preload.js",
  "logo": "logo.png",
  "pluginSetting": { "single": false },
  "features": [
    {
      "code": "qrcode",
      "explain": "二维码生成、二维码识别",
      "cmds": [
        "QRCode",
        "二维码",
        "扫码",
        {
          "type": "regex",
          "match": "/^https?:\\/\\/(?:(?:(?:25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]\\d|\\d)(?:\\.(?:25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]\\d|\\d)){3})|(?:(?:[a-z\\u00a1-\\uffff0-9]-*)*[a-z\\u00a1-\\uffff0-9]+)(?:\\.(?:[a-z\\u00a1-\\uffff0-9]-*)*[a-z\\u00a1-\\uffff0-9]+)*(?:\\.(?:[a-z\\u00a1-\\uffff]{2,}))\\.?)(?::\\d{2,5})?(?:[/?#][^\\s\"]*)?$/i",
          "label": "生成二维码"
        },
        { "type": "img", "label": "识别图片中二维码" }
      ]
    }
  ],
  "name": "QRCode",
  "pluginName": "二维码助手",
  "description": "二维码生成、二维码识别",
  "author": "福州猿力信息科技有限公司",
  "homepage": "https://u.tools",
  "version": "3.0.0"
}
