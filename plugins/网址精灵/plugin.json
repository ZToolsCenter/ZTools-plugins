{
    "pluginName": "网址精灵",
    "version": "2.1.11",
    "description": "一键收藏、搜索web网页，打造个人网站网址导航站、书签导入、网页快开神器！",
    "author": "呀唔咪",
    "homepage": null,
    "main": "index.html",
    "preload": "preload.js",
    "logo": "logo.png",
    "pluginSetting": {
        "height": 1
    },
    "features": [
        {
            "code": "goodweb",
            "explain": "一键收藏网页、打造个人网址导航站、网页快开神器！",
            "icon": "logo.png",
            "cmds": [
                "网址精灵",
                "网址导航",
                "网页收藏",
                "web"
            ]
        },
        {
            "code": "searchweb",
            "explain": "在网址精灵中搜索网址卡片！",
            "icon": "search.png",
            "cmds": [
                {
                    "type": "over",
                    "label": "搜索网址卡片",
                    "minLength": 2,
                    "maxLength": 20
                }
            ],
            "mainPush": true
        },
        {
            "code": "addweb",
            "explain": "一键快速添加网址卡片！",
            "icon": "logo.png",
            "cmds": [
                {
                    "type": "regex",
                    "label": "添加网址卡片",
                    "match": "/^(http|https|ftp|chrome|edge|mailto|file|data|ws|wss|irc|git|svn|utools):///"
                }
            ]
        }
    ],
    "platform": [
        "darwin",
        "win32",
        "linux"
    ],
    "name": "网址精灵"
}